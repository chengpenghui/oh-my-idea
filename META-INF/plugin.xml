<idea-plugin url="http://plugins.jetbrains.com/plugin/164">
    <name>Oh My IDEA</name>
    <id>oh-my-idea</id>
    <change-notes></change-notes>
    <description></description>
    <version>1.0</version>
    <vendor>IntelliJ</vendor>


    <application-components>
        <component>
            <implementation-class>com.codingbaby.ohmyidea.OhPlugin</implementation-class>
        </component>
    </application-components>


    <actions>
        <action id="MotionLeft" class="com.codingbaby.ohmyidea.action.MotionLeftAction" text="Left"/>
        <action id="MotionRight" class="com.codingbaby.ohmyidea.action.MotionRightAction" text="Right"/>
        <action id="MotionUp" class="com.codingbaby.ohmyidea.action.MotionUpAction" text="Up"/>
        <action id="MotionDown" class="com.codingbaby.ohmyidea.action.MotionDownAction" text="Down"/>
    </actions>


</idea-plugin>
